{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "pretty": true,
    "strict": false,
    "removeComments": false,
    "moduleResolution": "node",
    "paths": {
      "@probeh/ngx-container": [
        "dist/ngx-container/ngx-container",
        "dist/ngx-container"
      ],
      "@probeh/ngx-sourcemap": [
        "dist/ngx-sourcemap/ngx-sourcemap",
        "dist/ngx-sourcemap"
      ],
      "@probeh/ngx-monitor": [
        "dist/ngx-monitor/ngx-monitor",
        "dist/ngx-monitor"
      ],
      "ngx-restworker": [
        "dist/ngx-restworker/ngx-restworker",
        "dist/ngx-restworker"
      ]
    }
  }
}