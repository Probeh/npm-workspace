{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "defaultProject": "npm-workspace",
  "newProjectRoot": "projects",
  "projects": {
    "ngx-container": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-container/tsconfig.lib.prod.json"
            }
          },
          "options": {
            "project": "projects/ngx-container/ng-package.json",
            "tsConfig": "projects/ngx-container/tsconfig.lib.json"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "exclude": ["**/node_modules/**"],
            "tsConfig": [
              "projects/ngx-container/tsconfig.lib.json",
              "projects/ngx-container/tsconfig.spec.json"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "karmaConfig": "projects/ngx-container/karma.conf.js",
            "main": "projects/ngx-container/src/test.ts",
            "tsConfig": "projects/ngx-container/tsconfig.spec.json"
          }
        }
      },
      "prefix": "ngx",
      "projectType": "library",
      "root": "projects/ngx-container",
      "schematics": {
        "@schematics/angular:component": {
          "export": false,
          "skipImport": true,
          "skipTests": true,
          "style": "scss"
        },
        "@schematics/angular:directive": {
          "export": false,
          "skipImport": true,
          "skipTests": true
        },
        "@schematics/angular:module": {
          "commonModule": true,
          "routing": true,
          "routingScope": "Child",
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "sourceRoot": "projects/ngx-container/src"
    },
